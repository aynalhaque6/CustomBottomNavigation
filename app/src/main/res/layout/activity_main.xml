<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.aynal.cnb.CustomBottomNavigation
        android:id="@+id/customBottomNav"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        app:cbn_backgroundColor="@color/white"
        app:cbn_fabColor="#1B7D36"
        app:cbn_activeColor="#1B7D36"
        app:cbn_inactiveColor="#757575"

        app:cbn_fabSize="56dp"
        app:cbn_itemTextSize="10sp"
        app:cbn_itemIconSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>